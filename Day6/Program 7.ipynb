   "source": [
    "# Ackermann Function\n",
    "def Ackermann(m, n, s =\"% s\"): \n",
    "    print(s % (\"(% d, % d)\" % (m, n))) \n",
    "    if m == 0: \n",
    "        return n + 1\n",
    "    if n == 0: \n",
    "        return (m - 1, 1, s) \n",
    "    n2 = (m, n - 1, s % (\"(% d, %% s)\" % (m - 1))) \n",
    "    return (m - 1, n2, s) \n",
    "print(Ackermann(1, 2)) \n"
   ]
