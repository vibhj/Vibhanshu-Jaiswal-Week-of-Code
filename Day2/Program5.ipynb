   "source": [
    "n=int(input(\"Enter the Value of n : \"))\n",
    "for i in range(n):\n",
    "    print((str(n-i)+\"*\")*(n-1-i)+str(n-i))\n",
    "for i in range(2,n + 1):\n",
    "    print((str(i)+\"*\")*(i-1)+str(i))\n"
